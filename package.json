{
  "name": "@capacitor-community/electron",
  "description": "Containing project for Capacitor community support for the Electron platform.",
  "private": true,
  "scripts": {
    "publish": "lerna publish --force-publish --skip-git && npm run publish-push",
    "publish-push": "bash scripts/deploy.sh",
    "generate-changelog": "conventional-changelog -o CHANGELOG.md -r 0 --config changelog.config.js"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "directories": {
    "doc": "docs"
  },
  "author": "IT-MikeS",
  "dependencies": {
    "lerna": "^2.4.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^6.16.0",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-changelog-conventionalcommits": "^4.3.0"
  }
}
